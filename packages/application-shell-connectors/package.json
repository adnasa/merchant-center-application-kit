{
  "name": "@commercetools-frontend/application-shell-connectors",
  "version": "1.0.0-rc.2",
  "description": "Contains complementary tools for @commercetools-frontend/application-shell",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./src/index.js -o dist/application-shell-connectors.cjs.js",
    "build:es": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f es ./src/index.js -o dist/application-shell-connectors.es.js"
  },
  "main": "./dist/application-shell-connectors.cjs.js",
  "module": "./dist/application-shell-connectors.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "dependencies": {
    "@commercetools-frontend/constants": "1.0.0-rc.2",
    "lodash.get": "4.4.2",
    "moment-timezone": "0.5.21",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-apollo": "2.2.4",
    "recompose": "0.26.0"
  }
}
