{
  "name": "@commercetools-frontend/l10n",
  "version": "1.0.0-beta.0",
  "description": "React bindings to load l10n data",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "main": "dist/l10n.cjs.js",
  "module": "dist/l10n.es.js",
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "generate-data": "node ./scripts/generate-l10n-data.js",
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./index.js -o dist/l10n.cjs.js",
    "build:es": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f es ./index.js -o dist/l10n.es.js"
  },
  "dependencies": {
    "@commercetools-frontend/sentry": "1.0.0-beta.0",
    "moment": "2.22.2",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "recompose": "0.26.0"
  },
  "devDependencies": {
    "chalk": "2.4.1",
    "cldr": "4.11.0",
    "deep-diff": "1.0.2",
    "moment-timezone": "0.5.21",
    "node-fetch": "2.2.0",
    "rimraf": "2.6.2",
    "shelljs": "0.8.2"
  }
}
