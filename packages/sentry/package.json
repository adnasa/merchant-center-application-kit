{
  "name": "@commercetools-frontend/sentry",
  "version": "1.0.0-beta.0",
  "description": "Components to set up Sentry for a MC application",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./index.js -o ./dist/sentry.cjs.js",
    "build:es": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f es ./index.js -o ./dist/sentry.es.js"
  },
  "main": "./dist/sentry.cjs.js",
  "module": "./dist/sentry.es.js",
  "dependencies": {
    "prop-types": "15.6.2",
    "raven-js": "3.27.0",
    "react": "16.5.2"
  }
}
