{
  "name": "@commercetools-frontend/sdk",
  "version": "1.0.0-rc.2",
  "description": "Tools for declarative fetching",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./index.js -o ./dist/sdk.cjs.js",
    "build:es": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f es ./index.js -o ./dist/sdk.es.js"
  },
  "main": "./dist/sdk.cjs.js",
  "module": "./dist/sdk.es.js",
  "dependencies": {
    "@commercetools-frontend/actions-global": "1.0.0-rc.2",
    "@commercetools-frontend/constants": "1.0.0-rc.2",
    "@commercetools/api-request-builder": "4.0.2",
    "@commercetools/sdk-client": "1.5.8",
    "@commercetools/sdk-middleware-correlation-id": "1.0.1",
    "@commercetools/sdk-middleware-http": "4.0.4",
    "@commercetools/sdk-middleware-user-agent": "1.2.10",
    "fast-equals": "1.6.1",
    "prop-types": "15.6.2",
    "query-string": "5.1.0",
    "react": "16.5.2",
    "react-redux": "5.0.7",
    "unfetch": "3.1.1"
  }
}
