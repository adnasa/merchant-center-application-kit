{
  "name": "@commercetools-frontend/react-notifications",
  "version": "1.0.0-rc.2",
  "description": "React bindings for @commercetools-frontend/notifications",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./src/index.js -o ./dist/react-notifications.cjs.js",
    "build:es": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f es ./src/index.js -o ./dist/react-notifications.es.js"
  },
  "main": "./dist/react-notifications.cjs.js",
  "module": "./dist/react-notifications.es.js",
  "dependencies": {
    "@commercetools-frontend/actions-global": "1.0.0-rc.2",
    "@commercetools-frontend/constants": "1.0.0-rc.2",
    "@commercetools-frontend/notifications": "1.0.0-rc.2",
    "@commercetools-frontend/sentry": "1.0.0-rc.2",
    "@commercetools-frontend/ui-kit": "2.0.0-rc.11",
    "classnames": "2.2.6",
    "lodash.has": "4.5.2",
    "lodash.isnumber": "3.0.3",
    "moment": "2.22.2",
    "moment-timezone": "0.5.21",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-intl": "2.7.1",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "recompose": "0.26.0",
    "reselect": "3.0.1"
  }
}
