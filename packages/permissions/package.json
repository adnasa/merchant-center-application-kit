{
  "name": "@commercetools-frontend/permissions",
  "version": "1.0.0-beta.0",
  "description": "React components to declaratively handle MC permissions",
  "bugs": "https://github.com/commercetools/merchant-center-application-kit/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/commercetools/merchant-center-application-kit.git"
  },
  "keywords": [
    "javascript",
    "frontend",
    "react",
    "toolkit"
  ],
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist/**",
    "build": "cross-env npm run build:es && npm run build:cjs",
    "build:cjs": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f cjs ./src/index.js -o ./dist/permissions.cjs.js",
    "build:es": "cross-env NODE_ENV=development rollup -c ../../rollup.config.js  -f es ./src/index.js -o ./dist/permissions.es.js"
  },
  "main": "./dist/permissions.cjs.js",
  "module": "./dist/permissions.es.js",
  "dependencies": {
    "@commercetools-frontend/application-shell-connectors": "1.0.0-beta.0",
    "lodash.camelcase": "4.3.0",
    "lodash.isnil": "4.0.0",
    "lodash.upperfirst": "4.3.1",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "recompose": "0.26.0",
    "reselect": "3.0.1",
    "warning": "4.0.2"
  }
}
